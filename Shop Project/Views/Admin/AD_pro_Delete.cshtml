@model Product

@{
    Layout = "~/Views/Shared/Admin_Layout.cshtml";

}
<h1>Create</h1>
<form method="post" class="row g-3">
    <div class="col-md-6">
        <label asp-for="P_name" for="Name" class="form-label">Product</label>
        <input asp-for="P_name" class="form-control" id="Name">
    </div>
    <div class="col-md-8">
        <label asp-for="P_price" class="form-label">Price</label>
        <input asp-for="P_price" class="form-control" id="Price">
    </div>
    <div class="col-12">
        <button type="submit" class="btn btn-primary" asp-controller="Admin" asp-action="AD_pro_Delete">Delete</button>
    </div>
</form>
